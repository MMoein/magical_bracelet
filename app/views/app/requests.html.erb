<%= form_tag("/decide", method: "post") do %>
    <div class = 'container'>
      <div class="col-sm-5 col-sm-offset-3">
      </div>
      <%= hidden_field_tag :ev_id, @ev.id%>
      <div class="col-sm-2 col-sm-offset-3" >
        <%= submit_tag("Reject", :class => 'form-control') %>
      </div>
      <div class="col-sm-2">
        <%= submit_tag("Accept",:name => 'Accept', :class => 'form-control') %>
      </div>
    </div>
<%end%>